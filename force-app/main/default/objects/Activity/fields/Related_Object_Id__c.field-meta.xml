<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Related_Object_Id__c</fullName>
    <description>Derive the Salesforce ID for the associated object, using WhatId as a priority.</description>
    <externalId>false</externalId>
    <formula>IF(ISBLANK(WhatId),WhoId,WhatId)</formula>
    <label>Related Object Id</label>
    <required>false</required>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
